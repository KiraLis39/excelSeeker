<?xml version="1.1" encoding="UTF-8" standalone="no"?>

<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet author="KiraLis39" id="0000000001">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="parsed_data"/>
            </not>
        </preConditions>

        <createTable tableName="parsed_data">
            <column name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="parsed_data_pkey"/>
            </column>
            <column name="found_date" remarks="Дата обработки" type="TIMESTAMP WITHOUT TIME ZONE"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
